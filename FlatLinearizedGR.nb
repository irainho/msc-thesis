 RowBox[{
  RowBox[{
   RowBox[{"hField", "[", 
    RowBox[{"T", ",", "R", ",", "\[Theta]", ",", "\[Phi]"}], "]"}], " ", "=", 
   " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Delta]h", "+"], "[", 
     RowBox[{"T", ",", "R", ",", "\[Theta]", ",", "\[Phi]"}], "]"}], "+", 
    RowBox[{"I", " ", 
     RowBox[{
      SubscriptBox["\[Delta]h", "x"], "[", 
      RowBox[{"T", ",", "R", ",", "\[Theta]", ",", "\[Phi]"}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"SWLapHField", "=", " ", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"1", "/", 
      RowBox[{"Sin", "[", "\[Theta]", "]"}]}], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Theta]"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], 
         RowBox[{
          SubscriptBox["\[PartialD]", "\[Theta]"], 
          RowBox[{"hField", "[", 
           RowBox[{"T", ",", "R", ",", "\[Theta]", ",", "\[Phi]"}], "]"}]}]}],
         ")"}]}], ")"}]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"2", "-", 
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "I"}], " ", 
           RowBox[{
            SubscriptBox["\[PartialD]", "\[Phi]"], 
            RowBox[{"+", "2"}]}], 
           RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ")"}], "^", "2"}], 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "^", "2"}]]}], ")"}], 
     RowBox[{"hField", "[", 
      RowBox[{"T", ",", "R", ",", "\[Theta]", ",", "\[Phi]"}], "]"}]}]}], "//",
    "Simplify"}]}]}], "Input",
